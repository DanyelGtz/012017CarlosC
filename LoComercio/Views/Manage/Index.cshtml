@model IndexViewModel
@{
    ViewData["Title"] = "Administración";
}

<h2 class="titulo">@ViewData["Title"].</h2>

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Módulos</h3>
    </div>
    <div class="panel-body">
        <div class="container">
            <div class="row">
            <div class="col-sm-3">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Marcas</h3>
                    </div>
                    <div class="panel-body">
                        <div class="container">
                            <a asp-controller="Marcas" asp-action="Create" class="btn btn-primary">Registrar</a>
                            <a asp-controller="Marcas" asp-action="Index" class="btn btn-default">Ver</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Modelos</h3>
                    </div>
                    <div class="panel-body">
                        <div class="container">
                            <a asp-controller="Modelos" asp-action="Create" class="btn btn-primary">Registrar</a>
                            <a asp-controller="Modelos" asp-action="Index" class="btn btn-default">Ver</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Estados de Dispositivos</h3>
                    </div>
                    <div class="panel-body">
                        <div class="container">
                            <a asp-controller="EstadoDispositivos" asp-action="Create" class="btn btn-primary">Registrar</a>
                            <a asp-controller="EstadoDispositivos" asp-action="Index" class="btn btn-default">Ver</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Lugares de Almacenamiento</h3>
                    </div>
                    <div class="panel-body">
                        <div class="container">
                            <a asp-controller="LugarAlmacenamientos" asp-action="Create" class="btn btn-primary">Registrar</a>
                            <a asp-controller="LugarAlmacenamientos" asp-action="Index" class="btn btn-default">Ver</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-3">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Estados de Notificacion Cliente</h3>
                    </div>
                    <div class="panel-body">
                        <div class="container">
                            <a asp-controller="EstadoNotificaciones" asp-action="Create" class="btn btn-primary">Registrar</a>
                            <a asp-controller="EstadoNotificaciones" asp-action="Index" class="btn btn-default">Ver</a>
